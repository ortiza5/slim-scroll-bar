{
  "manifest_version": 3,
  "name": "Slim Scroll Bar",
  "description": "A less ugly scrollbar",
  "version": "1.1",
  "content_scripts": [
    {
      "css": ["style.css"],
      "matches": ["*://*/*"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
